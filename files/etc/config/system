config system
	option hostname 'CharlinhOS'
	option timezone 'UTC'
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option compat_version '1.0'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '0'
	list server 'a.st1.ntp.br'
	list server 'b.st1.ntp.br'
	list server 'a.ntp.br'
	list server 'b.ntp.br'

config led 'led_wifi_led'
	option name 'wifi'
	option sysfs 'green:wlan'
	option trigger 'netdev'
	option mode 'link tx rx'
	option dev 'wlan0'
